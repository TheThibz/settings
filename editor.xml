<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_LEADING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\bNotImplementedException\b" />
    <pattern useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO_Nano)(\W|$)(.*)">
      <option name="FOREGROUND" value="b24cd4" />
      <option name="FONT_TYPE" value="1" />
      <option name="ERROR_STRIPE_COLOR" value="b24cd4" />
    </pattern>
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG)(\W|$)(.*)" />
    <filter name="nano only">
      <pattern index="1" />
    </filter>
  </component>
</application>